{
  "name": "mine.js",
  "description": "an easy-to-use voxel engine",
  "version": "1.0.0",
  "main": "dist/mine.js",
  "module": "dist/mine.es.js",
  "umd": "dist/mine.min.js",
  "files": [
    "dist/**/*"
  ],
  "types": "dist/index.d.ts",
  "repository": "https://github.com/ian13456/mine.js.git",
  "author": "Ian Huang <21ianh1@students.tas.tw>",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint '*/**/*.{js,ts}' --quiet --fix",
    "test": "jest --config jestconfig.json",
    "prepare": "yarn run build",
    "prepublishOnly": "yarn test && yarn run lint",
    "preversion": "yarn run lint",
    "version": "yarn run format && git add -A src",
    "postversion": "git push && git push --tags",
    "build": "rollup -c",
    "watch": "rollup -cw",
    "dev": "gulp serve",
    "start": "concurrently \"yarn watch\" \"yarn dev\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "pretty-quick --staged"
    ]
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "@vercel/ncc": "^0.27.0",
    "browser-sync": "^2.26.14",
    "concurrently": "^5.3.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "gulp": "^4.0.2",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "rollup": "^2.38.4",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-jest": "^26.5.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.3"
  }
}
